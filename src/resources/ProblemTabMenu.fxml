<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<TabPane prefHeight="400.0" prefWidth="592.0" tabClosingPolicy="UNAVAILABLE" 
xmlns="http://javafx.com/javafx/8.0.141" 
xmlns:fx="http://javafx.com/fxml/1"
fx:controller="pt.iscte.es2.decisionsoft.gui.controllers.ProblemTabMenuController">
   <tabs>
      <Tab text="Problem Definition">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="268.0" prefWidth="573.0">
               <children>
                  <Label layoutX="14.0" layoutY="24.0" text="Problem Name: " />
                  <TextField fx:id="problemNameTextField" layoutX="100.0" layoutY="20.0" prefHeight="25.0" prefWidth="475.0" />
                  <Label layoutX="14.0" layoutY="58.0" text="Problem Description" />
                  <TextArea fx:id="problemDescriptionTextArea" layoutX="14.0" layoutY="75.0" prefHeight="200.0" prefWidth="558.0" />
                  <Label layoutX="14.0" layoutY="292.0" text="User email: " />
                  <TextField fx:id="userEmailTextField" layoutX="76.0" layoutY="288.0" prefHeight="25.0" prefWidth="497.0" />
                  <Button fx:id="backButton" layoutX="464.0" layoutY="332.0" mnemonicParsing="false" onAction="#handleBack" text="&lt; Back" />
                  <Button fx:id="nextButton" layoutX="526.0" layoutY="332.0" mnemonicParsing="false" onAction="#handleNext" text="Next &gt;" />
               </children>
            </AnchorPane>
         </content>
      </Tab>
      <Tab text="Time Preferences">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
         </content>
      </Tab>
      <Tab text="Variables">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
         </content>
      </Tab>
      <Tab text="Evaluation Mode">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
         </content>
      </Tab>
      <Tab text="Algorithm">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
         </content>
      </Tab>
      <Tab text="FAQ">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
         </content>
      </Tab>
      <Tab text="Contact Admin">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="268.0" prefWidth="573.0" />
         </content>
      </Tab>
   </tabs>
</TabPane>
